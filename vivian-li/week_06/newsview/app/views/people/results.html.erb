<% unless @people.empty? %>

        <% @people.each do |person| %>
         <p><strong><%= person.name %></strong></p>
        <p><%= link_to(image_tag(person.image), person) %></p>
            <% end %>
            <% else  %>
        <h3>No results found please try again </h3>
        <% end %>


