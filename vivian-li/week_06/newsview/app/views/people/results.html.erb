<% unless @people.empty? %>
    <ul>
        <% @people.each do |person| %>
        <li>
            <ul>
                <li>
                    <%= person.name %>
                </li>
                <li>
                    <%= link_to(image_tag(person.image), person) %>
                </li>
            </ul>
        </li>
        <% end %>
    </ul>
<% else  %>
    <h3>No results found!!! </h3>
<% end %>


