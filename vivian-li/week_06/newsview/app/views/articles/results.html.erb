
<div class="col-md-8 col-md-offset-1">
<% unless @articles.empty? %>
  <% @articles.each do |article| %>
      <p><strong><%= link_to article.headline_english, article %></strong></p>
       <p><%= link_to(image_tag(article.screen_capture), article)%></p>
       <p><%= article.own_notes %></p>
       <br>
           <% end %>
            <% else  %>
   <h3>No results found please try again</h3>
    <% end %>



