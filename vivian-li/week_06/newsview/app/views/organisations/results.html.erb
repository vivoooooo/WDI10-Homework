<% unless @organisations.empty? %>

    <% @organisations.each do |organisation| %>
      <p><strong><%=link_to organisation.name, organisation %></strong></p>
        <p><%= link_to(image_tag(organisation.image), organisation) %></p>
     <% end %>
     <% else  %>
    <h3>No results found please try again</h3>
   <% end %>




